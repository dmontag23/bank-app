    "start": "react-native start",
    "test:unit": "jest --silent",
    "test:e2e": "detox test -c $E2E_CONFIG",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
