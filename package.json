{
  "name": "bank-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean-mock-server": "rm -rf ./tests/mocks/server/dist",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "mock-server": "npm run clean-mock-server && npm run mock-ts-compile && node tests/mocks/server/dist/tests/mocks/server/runMockServer.js",
