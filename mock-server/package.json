{
  "name": "bank-app-mock-dev-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run install",
    "clean": "npm run clean-node-modules && npm run clean-mock-server",
    "clean-mock-server": "rm -rf dist",
    "clean-node-modules": "rm -rf node_modules",
    "install": "npm install && npm run mock-ts-compile",
    "mock-ts-compile": "npx tsc",
    "start": "npm run build && node dist/mock-server/runMockServer.js"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/express": "^4.17.17",
    "typescript": "4.9.5"
  }
}
